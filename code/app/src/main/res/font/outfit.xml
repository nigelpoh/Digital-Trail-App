<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <font
        android:fontWeight="400"
        android:fontStyle="normal"
        android:font="@font/outfit_regular"
        app:fontWeight="400"
        app:fontStyle="normal"
        app:font="@font/outfit_regular"/>

    <font
        android:fontWeight="500"
        android:fontStyle="normal"
        android:font="@font/outfit_medium"
        app:fontWeight="500"
        app:fontStyle="normal"
        app:font="@font/outfit_medium"/>

    <font
        android:fontWeight="600"
        android:fontStyle="normal"
        android:font="@font/outfit_semibold"
        app:fontWeight="600"
        app:fontStyle="normal"
        app:font="@font/outfit_semibold" />

    <font
        android:fontWeight="700"
        android:fontStyle="normal"
        android:font="@font/outfit_bold"
        app:fontWeight="700"
        app:fontStyle="normal"
        app:font="@font/outfit_bold" />

    <font
        android:fontWeight="800"
        android:fontStyle="normal"
        android:font="@font/outfit_extrabold"
        app:fontWeight="800"
        app:fontStyle="normal"
        app:font="@font/outfit_extrabold" />

    <font
        android:fontWeight="900"
        android:fontStyle="normal"
        android:font="@font/outfit_black"
        app:fontWeight="900"
        app:fontStyle="normal"
        app:font="@font/outfit_black" />

</font-family>